{
    "manifest_version": 2,
    "name": "Twitter to TweetDeck Redirect",
    "version": "1.0",
    "description": "Redirects twitter.com to tweetdeck.twitter.com",
    "permissions": ["webRequest", "webRequestBlocking", "*://twitter.com/*"],
    "background": {
      "scripts": ["background.js"],
      "persistent": true
    }
}
